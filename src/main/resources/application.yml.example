spring:
    profiles:
        active: ${APP_PROFILE:test}
    application:
    name: "Fileflow"
    security:
        token:
            secret: ${FILE_FLOW_JWT_SECRET:12345678}
    flyway:
      enabled: true
    jpa:
    properties:
        hibernate:
            show_sql: true
            format_sql: true
            ddl-auto: none
            dialect: org.hibernate.dialect.PostgreSQLDialect
integrations:
    ses:
        access_key: 111111
        secret_key: 111111